    "test":         "NODE_ENV=production NODE_PATH=lib node --harmony test/index.node.js; open test/index.html",
    "lint":         "plato --title Valid.js --jshint .jshintrc -d lint/plato lib/Valid.js; open lint/plato/index.html"
